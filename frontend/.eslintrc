{
  "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
